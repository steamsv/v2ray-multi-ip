{
    "inbound": {https://github.com/steamsv/ssduo/tree/main
        "allocate": {
            "strategy": "always"
        },
        "listen": "0.0.0.0",
        "port": 8090,
        "protocol": "vmess",
        "settings": {
            "clients": [
                {
                    "id": "112a9d28-351f-df4c-7747-9f3e14a9ba12",
